{
  "name": "@texoport/parseme",
  "version": "0.1.1",
  "description": "A parser combinator library inspired by Parsec and Effect-TS",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./combinators": {
      "types": "./dist/combinators.d.ts",
      "default": "./dist/combinators.js"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "default": "./dist/utils.js"
    },
    "./state": {
      "types": "./dist/state.d.ts",
      "default": "./dist/state.js"
    },
    "./parser": {
      "types": "./dist/parser.d.ts",
      "default": "./dist/parser.js"
    },
    "./lexer": {
      "types": "./dist/lexer.d.ts",
      "default": "./dist/lexer.js"
    },
    "./either": {
      "types": "./dist/either.d.ts",
      "default": "./dist/either.js"
    },
    "./debug": {
      "types": "./dist/debug.d.ts",
      "default": "./dist/debug.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "test": "bun test",
    "prepublishOnly": "bun run build && bun test",
    "publish:patch": "npm version patch && npm publish --access public",
    "publish:minor": "npm version minor && npm publish --access public",
    "publish:major": "npm version major && npm publish --access public",
    "release:patch": "bun run build && bun test && bun run publish:patch",
    "release:minor": "bun run build && bun test && bun run publish:minor",
    "release:major": "bun run build && bun test && bun run publish:major"
  },
  "keywords": [
    "parser",
    "parser-combinators",
    "typescript",
    "functional-programming"
  ],
  "author": "texoport",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/bun": "^1.1.10",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "typescript": "^5.7.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/texoport/parseme.git"
  },
  "bugs": {
    "url": "https://github.com/texoport/parseme/issues"
  },
  "homepage": "https://github.com/texoport/parseme#readme"
}
